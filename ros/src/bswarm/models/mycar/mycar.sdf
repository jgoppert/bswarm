<?xml version='1.0'?>
<sdf version="1.6">
<model name="mycar">
  <pose>0 0 1.0 0 0 0</pose>
    <link name="chasis">
      <inertial>
        <mass>1.0</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>0.1</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>0.1</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0</iyz>         <!-- for a box: iyz = 0 -->
          <izz>0.1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>1 1 1</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
				<material>
					<ambient>0 0 0</ambient>
				</material>
        <geometry>
          <box>
            <size>1 1 1</size>
          </box>
        </geometry>
      </visual>
    </link>

    <link name="front_left_wheel">
      <pose>0.5 0.7 -0.5 1.57 0 0</pose>
      <inertial>
        <mass>1.0</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>0.01</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>0.01</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0</iyz>         <!-- for a box: iyz = 0 -->
          <izz>0.1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.1</length>
          </cylinder> 
        </geometry>
      </collision>
      <visual name="visual">
				<material>
					<ambient>1 0 0</ambient>
				</material>
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.1</length>
          </cylinder> 
        </geometry>
      </visual>
    </link>

    <joint name="front_left_axle" type="revolute2">
			<parent>chasis</parent> 
			<child>front_left_wheel</child> 
			<axis>
				<limit>
					<lower>-0.2</lower>
					<upper>0.2</upper>
				</limit>
				<xyz>0 0 1</xyz>
			</axis>
			<axis2>
				<xyz>0 1 0</xyz>
			</axis2>
    </joint>

    <link name="front_right_wheel">
      <pose>0.5 -0.7 -0.5 1.57 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>0.01</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>0.01</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0</iyz>         <!-- for a box: iyz = 0 -->
          <izz>0.1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.1</length>
          </cylinder> 
        </geometry>
      </collision>
      <visual name="visual">
				<material>
					<ambient>1 0 0</ambient>
				</material>
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.1</length>
          </cylinder> 
        </geometry>
      </visual>
    </link>

    <joint name="front_right_axle" type="revolute2">
			<parent>chasis</parent> 
			<child>front_right_wheel</child> 
			<axis>
				<limit>
					<lower>-0.2</lower>
					<upper>0.2</upper>
				</limit>
				<xyz>0 0 1</xyz>
			</axis>
			<axis2>
				<xyz>0 1 0</xyz>
			</axis2>
    </joint>
 
    <link name="rear_left_wheel">
      <pose>-0.5 0.7 -0.5 1.57 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>0.01</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>0.01</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0</iyz>         <!-- for a box: iyz = 0 -->
          <izz>0.1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.1</length>
          </cylinder> 
        </geometry>
      </collision>
      <visual name="visual">
				<material>
					<ambient>0 0 1</ambient>
				</material>
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.1</length>
          </cylinder> 
        </geometry>
      </visual>
    </link>

    <joint name="rear_left_axle" type="revolute">
			<parent>chasis</parent> 
			<child>rear_left_wheel</child> 
			<axis>
				<xyz>0 0 1</xyz>
			</axis>
    </joint>

    <link name="rear_right_wheel">
      <pose>-0.5 -0.7 -0.5 1.57 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>0.01</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>0.01</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0</iyz>         <!-- for a box: iyz = 0 -->
          <izz>0.1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.2</radius>
